<launch>

    <!-- telep modality -->
    <arg name="teleop_config" default="$(find pad_control)/config/full_task_space.yaml" />

    <!-- teleop node -->
    <rosparam file="$(arg teleop_config)" command="load" />
    <node pkg="joy_teleop" type="joy_teleop.py" name="joy_teleop"/>

    <!-- kuka controller node -->
    <group ns="kuka_controller">
        <node pkg="pad_control" type="controller.py" name="controller"/>
    </group>

    <!-- joy node
    <node respawn="true" pkg="joy" type="joy_node" >
        <param name="dev" type="string" value="/dev/input/js0" />
        <param name="deadzone" value="0.12" />
    </node>
    -->

    <!-- joy teleop node
    <node pkg="joy_teleop" type="joy_teleop.py" name="joy_teleop" />
    -->

    <!-- Kuka controller Node
    <node pkg="pad_control" type="controller.py" name="controller_node"  output="screen"/>
    -->

</launch>
